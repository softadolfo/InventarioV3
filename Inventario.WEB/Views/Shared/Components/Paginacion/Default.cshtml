@model Inventario.WEB.Models.PagingVm


<ul class="pagination justify-content-center">
    @if (Model.UrlPaginaAnterior != null)
    {
        <li class="page-item"><a class="page-link" href="@Model.UrlPaginaAnterior" aria-label="Previous"> <span aria-hidden="true">&laquo;</span> <span class="sr-only">Previous</span> </a> </li>
    }
    @foreach (var item in Model.Botones)
    {
        <li class="page-item @if(item.Pagina == Model.PaginaActiva){@("active")}"><a class="page-link" href="@item.UrlBoton">@item.Pagina</a></li>
    }
    @if (Model.UrlPaginaSiguiente != null)
    {
        <li class="page-item"><a class="page-link" href="@Model.UrlPaginaSiguiente" aria-label="Next"> <span aria-hidden="true">&raquo;</span> <span class="sr-only">Next</span> </a> </li>
    }

</ul>
